<h1>Add your own book</h1>
<div class="form">
  <div class="row justify-content-center">
    <%= simple_form_for(@book) do |f| %>
    <%= f.input :title %>
    <%= f.input :genre %>
    <%#= f.input :cover %>
    <%= f.input :author %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
  </div>


</div>


